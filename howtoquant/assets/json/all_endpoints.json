{
    "endpoints":{
    "staticdata":{
        "GET /api/equities/all":{
            "description":"returns a list of all equities in users's organization",
            "queries":[],
            "requestBody":{},
            "exampleResponse":{
                "equities": [
                    {
                    "id": 1,
                    "name": "Tesla Inc",
                    "short_name": "Tesla",
                    "issuer": "Tesla",
                    "base_ccy": "USD",
                    "domicile": "USA"
                    }]
                }
            },
        "GET /api/idenitifiers/all":{
            "description":"returns a list of all identifiers in users's organization",
            "queries":[],
            "requestBody":{},
            "exampleResponse":{
                "identifiers": [
                    {
                    "id": 1,
                    "code": "TSLA",
                    "type": "BBG Ticker",
                    "inst_id": 1,
                    "instrument": "Tesla"
                    }]
                }
            }  
        },    
    "marketdata":{
        "PUT /api/prices/new":{
            "description":"triggers download of daily market prices and volume from Yahoo Finance for ticker and period and returns download id",
            "queries":[],
            "requestBody":{
                "tickers":["TSLA","VOD"],
                "date_from": "2024-03-01",
                "date_to": "2024-04-01"
            },
            "exampleResponse":{
                "download_id": 5
                }
            },
        "GET /api/prices/download/:downloadId" :{
            "description":"returns price fields saved by the identified download",
            "queries":[],
            "requestBody":{},
            "exampleResponse":{
                "prices": [{
                        "value_date": "2024-03-01",
                        "bid_price": 8.95,
                        "ask_price": 8.95,
                        "ticker": "VOD",
                        "value_field": "Open",
                        "source": "Yahoo Finance Prices"
                    }]
                }
            }
        }
        
    }
}